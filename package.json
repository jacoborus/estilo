{
  "name": "estilo",
  "version": "0.12.1",
  "description": "Generate vim color schemes without the hassle",
  "main": "estilo.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "hexterm": "^1.2.0",
    "js-yaml": "^3.6.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "gitbook-cli": "^2.3.0",
    "gitbook-plugin-github": "^2.0.0",
    "rimraf": "^2.5.2",
    "standard": "^7.1.2",
    "tape": "^4.6.0"
  },
  "scripts": {
    "test": "node test/index.js",
    "linter": "standard src/*.js estilo.js test bin/estilo",
    "postinstall": "node scripts/postinstall.js",
    "docs:publish": "rimraf _book && gitbook build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && git add . && git commit -am 'update book' && git push git@github.com:jacoborus/estilo gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jacoborus/estilo.git"
  },
  "bugs": {
    "url": "https://github.com/jacoborus/estilo/issues"
  },
  "homepage": "https://github.com/jacoborus/estilo",
  "engines": {
    "node": ">=5.9.0"
  },
  "keywords": [
    "vim",
    "color",
    "scheme",
    "schemes",
    "themes",
    "generator"
  ],
  "author": "Jacobo Tabernero http://jacoborus.codes",
  "license": "MIT"
}
